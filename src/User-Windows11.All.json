{
    "MinimumBuild": "10.0.22000",
    "MaximumBuild": "10.0.29999",
    "Registry": {
        "Type": "DefaultProfile",
        "Set": [
            {
                "path": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Advanced",
                "name": "TaskbarDa",
                "value": 0,
                "type": "DWord",
                "note": "Remove the Widgets icon from the Taskbar"
            },
            {
                "path": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Advanced",
                "name": "TaskbarMn",
                "value": 0,
                "type": "DWord",
                "note": "Remove the Chat icon from the Taskbar"
            }
        ],
        "SetOthers": [
            {
                "path": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Advanced",
                "name": "TaskbarAl",
                "value": 0,
                "type": "DWord",
                "note": "Aligns the Windows 11 Start button to the left of the Taskbar"
            },
            {
                "path": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Advanced",
                "name": "ShowTaskViewButton",
                "value": 0,
                "type": "DWord",
                "note": "Removes the Windows 11 Task View button on the Taskbar"
            }
        ]
    }
}