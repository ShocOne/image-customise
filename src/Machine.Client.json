{
    "MinimumBuild": "10.0.14393",
    "Registry": {
        "Set": [
            {
                "path": "HKLM:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer",
                "name": "DisableEdgeDesktopShortcutCreation",
                "value": 1,
                "type": "DWord"
            }
        ]
    },
    "Files": {
        "Copy": [
            {
                "Source": "Remove-AppxApps.ps1",
                "Destination": "C:\\ProgramData\\FeatureUpdates\\Remove-AppxApps.ps1"
            },
            {
                "Source": "SetupComplete.cmd",
                "Destination": "C:\\ProgramData\\FeatureUpdates\\SetupComplete.cmd"
            }
        ]
    }
}